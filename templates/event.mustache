{{#logo}}
    <img src="{{logo}}" alt="logo" style="float: right;" />
{{/logo}}

<h1>{{name}}</h1>
<p>{{#str}} event:submission:open, confman {{/str}} {{_submissionstart}} - {{_submissionend}}</p>
<p>{{#str}} event:submission:link, confman {{/str}}
        <a href="{{submissionlink}}" target="_blank">
            {{submissionlink}}
        </a>
</p>
<div>{{{intro}}}</div>
<p>{{#str}} event:organizer, confman {{/str}}: {{event_organizer}}</p>
<p>{{#str}} event:contact, confman {{/str}}: {{event_contact}}</p>
<h3>{{#str}} code:embed, confman{{/str}}</h3>
<pre>&lt;iframe src="{{submissionlink}}"&gt;&lt;/iframe&gt;</pre>
